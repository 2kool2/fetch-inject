{
  "name": "fetch-inject",
  "version": "0.0.0-development",
  "description": "Inline assets into the DOM using Fetch Injection.",
  "main": "lib/fetch-inject.js",
  "scripts": {
    "build": "npm run clean && rollup -c && BUILD_TARGET=minified rollup -c",
    "build-min": "BUILD_TARGET=minified rollup -c",
    "clean": "rimraf ./dist/*",
    "test": "mocha -r jsdom-global/register -r reify test",
    "lint": "standard",
    "semantic-release": "semantic-release pre && npm publish && semantic-release post"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/jhabdas/fetch-inject.git"
  },
  "keywords": [
    "dom",
    "fetch",
    "inject",
    "promise",
    "script",
    "css"
  ],
  "author": "Josh Habdas",
  "license": "MIT",
  "devDependencies": {
    "jsdom": "9.12.0",
    "jsdom-global": "2.1.1",
    "mocha": "^3.2.0",
    "reify": "^0.4.16",
    "rimraf": "^2.6.1",
    "rollup": "^0.41.4",
    "rollup-plugin-license": "^0.3.0",
    "rollup-plugin-uglify": "^1.0.1",
    "semantic-release": "^6.3.2",
    "standard": "^9.0.0",
    "uglify-js-harmony": "^2.7.5"
  },
  "standard": {
    "ignore": [
      "/dist"
    ]
  },
  "bugs": {
    "url": "https://github.com/jhabdas/fetch-inject/issues"
  },
  "homepage": "https://github.com/jhabdas/fetch-inject#readme"
}
